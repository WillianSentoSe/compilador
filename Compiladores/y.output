Gramática

    0 $accept: S $end

    1 S: TK_TIPO TK_MAIN '(' ')' BLOCO

    2 $@1: %empty

    3 BLOCO: '{' $@1 COMANDOS '}'

    4 COMANDOS: COMANDO COMANDOS
    5         | %empty

    6 COMANDO: CMD_DECLARACOES ';'
    7        | CMD_IF
    8        | CMD_WHILE
    9        | CMD_DOWHILE
   10        | CMD_FOR
   11        | CMD_SWITCH
   12        | CMD_BREAK
   13        | CMD_CONTINUE
   14        | CMD_OUT
   15        | '#'

   16 COMANDOS_FOR: COMANDO_FOR ',' COMANDOS_FOR
   17             | COMANDO_FOR

   18 COMANDO_FOR: ATRIBUICAO

   19 EXP_SIMPLES: TK_LITERAL
   20            | TK_ID
   21            | '(' EXPRESSAO ')'
   22            | TK_TIPO '(' EXPRESSAO ')'
   23            | TK_IN '(' TK_TIPO ')'

   24 EXP_POSFIXA: EXP_SIMPLES
   25            | EXP_POSFIXA '[' EXPRESSAO ']'

   26 EXP_UNARIA: EXP_POSFIXA
   27           | TK_OP_ADD EXP_POSFIXA
   28           | TK_OP_UN_ADD EXP_POSFIXA
   29           | TK_OP_UN_SUB EXP_POSFIXA
   30           | EXP_POSFIXA TK_OP_UN_ADD
   31           | EXP_POSFIXA TK_OP_UN_SUB

   32 EXP_NOT: EXP_UNARIA
   33        | TK_OP_NOT EXP_NOT

   34 EXP_ARITMETICA_MUL: EXP_NOT
   35                   | EXP_ARITMETICA_MUL TK_OP_MUL EXP_NOT

   36 EXP_ARITMETICA_ADD: EXP_ARITMETICA_MUL
   37                   | EXP_ARITMETICA_ADD TK_OP_ADD EXP_ARITMETICA_MUL

   38 EXP_RELACIONAL: EXP_ARITMETICA_ADD
   39               | EXP_RELACIONAL OP_RELACIONAL EXP_ARITMETICA_ADD

   40 EXP_IGUALDADE: EXP_RELACIONAL
   41              | EXP_IGUALDADE TK_OP_IGUALDADE EXP_RELACIONAL

   42 EXP_LOGICAL_AND: EXP_IGUALDADE
   43                | EXP_LOGICAL_AND TK_OP_LOGICAL_AND EXP_IGUALDADE

   44 EXP_LOGICAL_OR: EXP_LOGICAL_AND
   45               | EXP_LOGICAL_OR TK_OP_LOGICAL_OR EXP_LOGICAL_AND

   46 EXPRESSAO: EXP_LOGICAL_OR

   47 ATRIBUICAO: TK_ID '=' EXPRESSAO
   48           | TK_ID '[' EXPRESSAO ']' '=' EXPRESSAO

   49 CMD_DECLARACOES: TK_VAR LIST_DECLARACOES
   50                | ATRIBUICAO

   51 LIST_DECLARACOES: DECLARACAO ',' LIST_DECLARACOES
   52                 | DECLARACAO

   53 DECLARACAO: TK_ID
   54           | TK_ID '=' EXPRESSAO
   55           | TK_ID '[' TK_LITERAL ']'

   56 CMD_IF: TK_IF '(' EXPRESSAO ')' BLOCO
   57       | TK_IF '(' EXPRESSAO ')' BLOCO TK_ELSE BLOCO

   58 $@2: %empty

   59 CMD_WHILE: TK_WHILE '(' EXPRESSAO ')' $@2 BLOCO

   60 $@3: %empty

   61 CMD_DOWHILE: TK_DO $@3 BLOCO TK_WHILE '(' EXPRESSAO ')'

   62 @4: %empty

   63 CMD_FOR: TK_FOR @4 '(' CMD_DECLARACOES ';' EXPRESSAO ';' COMANDOS_FOR ')' BLOCO

   64 $@5: %empty

   65 CMD_SWITCH: TK_SWITCH '(' TK_ID ')' $@5 '{' BLOCO_SWITCH '}'

   66 BLOCO_SWITCH: BLOCO_SWITCH EXP_CASE
   67             | %empty

   68 EXP_CASE: TK_CASE EXPRESSAO BLOCO
   69         | TK_DEFAULT BLOCO

   70 CMD_BREAK: TK_BREAK ';'

   71 CMD_CONTINUE: TK_CONTINUE ';'

   72 CMD_OUT: TK_OUT '(' EXPRESSAO ')' ';'

   73 OP_RELACIONAL: '<'
   74              | '>'
   75              | TK_OP_MAIORIGUAL
   76              | TK_OP_MENORIGUAL


