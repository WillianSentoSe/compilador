   47 $@2: %empty

   48 $@3: %empty

   49 CMD_FOR: TK_FOR $@2 TK_ID '(' EXPRESSAO ',' OP_COMPARATIVOS EXPRESSAO ')' BLOCO $@3

   50 $@4: %empty

   51 CMD_SWITCH: TK_SWITCH '(' TK_ID ')' $@4 '{' BLOCO_SWITCH '}'

   52 BLOCO_SWITCH: BLOCO_SWITCH EXP_CASE
