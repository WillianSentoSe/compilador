   19 EXP_ARITMETICA_ADD: EXP_ARITMETICA_MUL
   20                   | EXP_ARITMETICA_ADD TK_OP_ADD EXP_ARITMETICA_MUL

   21 EXP_RELACIONAL: EXP_ARITMETICA_ADD
   22               | EXP_RELACIONAL OP_RELACIONAL EXP_ARITMETICA_ADD

   23 EXP_IGUALDADE: EXP_RELACIONAL
   24              | EXP_IGUALDADE TK_OP_IGUALDADE EXP_RELACIONAL

   25 EXP_LOGICAL_AND: EXP_IGUALDADE
   26                | EXP_LOGICAL_AND TK_OP_LOGICAL_AND EXP_IGUALDADE

   27 EXP_LOGICAL_OR: EXP_LOGICAL_AND
   28               | EXP_LOGICAL_OR TK_OP_LOGICAL_OR EXP_LOGICAL_AND

   29 EXPRESSAO: EXP_LOGICAL_OR

   30 ATRIBUICAO: TK_ID '=' EXPRESSAO

   31 CMD_DECLARACOES: TK_VAR LIST_DECLARACOES
   32                | ATRIBUICAO

   33 LIST_DECLARACOES: DECLARACAO ',' LIST_DECLARACOES
   34                 | DECLARACAO

   35 DECLARACAO: TK_ID
   36           | TK_ID '=' EXPRESSAO

   37 CMD_IF: TK_IF '(' EXPRESSAO ')' BLOCO
   38       | TK_IF '(' EXPRESSAO ')' BLOCO TK_ELSE BLOCO

   39 CMD_WHILE: TK_WHILE '(' EXPRESSAO ')' BLOCO

   40 CMD_DOWHILE: TK_DO BLOCO TK_WHILE '(' EXPRESSAO ')'

   41 CMD_FOR: TK_FOR '(' CMD_DECLARACOES ';' EXPRESSAO ';' CMD_DECLARACOES ')' BLOCO
   42        | TK_FOR TK_ID '(' EXPRESSAO ',' OP_COMPARATIVOS EXPRESSAO ')' BLOCO

   43 CMD_SWITCH: TK_SWITCH '(' TK_ID ')' '{' BLOCO_SWITCH '}'

   44 BLOCO_SWITCH: BLOCO_SWITCH EXP_CASE
   45             | %empty

   46 EXP_CASE: TK_CASE TK_LITERAL BLOCO
   47         | TK_DEFAULT BLOCO

   48 OP_RELACIONAL: '<'
   49              | '>'
   50              | TK_OP_MAIORIGUAL
   51              | TK_OP_MENORIGUAL

   52 OP_COMPARATIVOS: OP_RELACIONAL
   53                | TK_OP_IGUALDADE
   54                | TK_OP_DESIGUALDADE


