   48 $@2: %empty

   49 $@3: %empty

   50 CMD_FOR: TK_FOR $@2 TK_ID '(' EXPRESSAO ',' OP_COMPARATIVOS EXPRESSAO ')' BLOCO $@3

   51 $@4: %empty

   52 CMD_SWITCH: TK_SWITCH '(' TK_ID ')' $@4 '{' BLOCO_SWITCH '}'

   53 BLOCO_SWITCH: BLOCO_SWITCH EXP_CASE
