Gramática

    0 $accept: S $end

    1 S: TK_TIPO TK_MAIN '(' ')' BLOCO

    2 $@1: %empty

    3 BLOCO: '{' $@1 COMANDOS '}'

    4 COMANDOS: COMANDO COMANDOS
    5         | %empty

    6 COMANDO: CMD_DECLARACOES ';'
    7        | CMD_IF
    8        | CMD_WHILE
    9        | CMD_DOWHILE
   10        | CMD_FOR
   11        | CMD_SWITCH
   12        | CMD_BREAK
   13        | CMD_CONTINUE
   14        | CMD_OUT
   15        | CMD_UNARIO ';'
   16        | '#'

   17 COMANDOS_FOR: COMANDO_FOR ',' COMANDOS_FOR
   18             | COMANDO_FOR

   19 COMANDO_FOR: ATRIBUICAO
   20            | CMD_UNARIO

   21 EXP_SIMPLES: TK_LITERAL
   22            | TK_ID
   23            | '(' EXPRESSAO ')'
   24            | TK_TIPO '(' EXPRESSAO ')'
   25            | TK_IN '(' TK_TIPO ')'
   26            | TK_IN '(' ')'

   27 EXP_POSFIXA: EXP_SIMPLES
   28            | EXP_POSFIXA '[' EXPRESSAO ']'

   29 EXP_UNARIA: EXP_POSFIXA
   30           | TK_OP_ADD EXP_POSFIXA
   31           | TK_OP_UN_ADD EXP_POSFIXA
   32           | TK_OP_UN_SUB EXP_POSFIXA
   33           | EXP_POSFIXA TK_OP_UN_ADD

   34 EXP_NOT: EXP_UNARIA
   35        | TK_OP_NOT EXP_NOT

   36 EXP_ARITMETICA_MUL: EXP_NOT
   37                   | EXP_ARITMETICA_MUL TK_OP_MUL EXP_NOT

   38 EXP_ARITMETICA_ADD: EXP_ARITMETICA_MUL
   39                   | EXP_ARITMETICA_ADD TK_OP_ADD EXP_ARITMETICA_MUL

   40 EXP_RELACIONAL: EXP_ARITMETICA_ADD
   41               | EXP_RELACIONAL OP_RELACIONAL EXP_ARITMETICA_ADD

   42 EXP_IGUALDADE: EXP_RELACIONAL
   43              | EXP_IGUALDADE TK_OP_IGUALDADE EXP_RELACIONAL

   44 EXP_LOGICAL_AND: EXP_IGUALDADE
   45                | EXP_LOGICAL_AND TK_OP_LOGICAL_AND EXP_IGUALDADE

   46 EXP_LOGICAL_OR: EXP_LOGICAL_AND
   47               | EXP_LOGICAL_OR TK_OP_LOGICAL_OR EXP_LOGICAL_AND

   48 EXPRESSAO: EXP_LOGICAL_OR

   49 ATRIBUICAO: TK_ID '=' EXPRESSAO
   50           | TK_ID '[' EXPRESSAO ']' '=' EXPRESSAO
   51           | TK_ID TK_OP_ADD '=' EXPRESSAO
   52           | TK_ID TK_OP_MUL '=' EXPRESSAO

   53 CMD_UNARIO: TK_OP_UN_ADD TK_ID
   54           | TK_OP_UN_SUB TK_ID
   55           | TK_ID TK_OP_UN_ADD
   56           | TK_ID TK_OP_UN_SUB

   57 CMD_DECLARACOES: TK_VAR LIST_DECLARACOES
   58                | ATRIBUICAO

   59 LIST_DECLARACOES: DECLARACAO ',' LIST_DECLARACOES
   60                 | DECLARACAO

   61 DECLARACAO: TK_ID
   62           | TK_ID '=' EXPRESSAO
   63           | TK_ID '[' TK_LITERAL ']'

   64 CMD_IF: TK_IF '(' EXPRESSAO ')' BLOCO
   65       | TK_IF '(' EXPRESSAO ')' BLOCO TK_ELSE BLOCO

   66 $@2: %empty

   67 CMD_WHILE: TK_WHILE '(' EXPRESSAO ')' $@2 BLOCO

   68 $@3: %empty

   69 CMD_DOWHILE: TK_DO $@3 BLOCO TK_WHILE '(' EXPRESSAO ')'

   70 @4: %empty

   71 CMD_FOR: TK_FOR @4 '(' CMD_DECLARACOES ';' EXPRESSAO ';' COMANDOS_FOR ')' BLOCO

   72 $@5: %empty

   73 CMD_SWITCH: TK_SWITCH '(' TK_ID ')' $@5 '{' BLOCO_SWITCH '}'

   74 BLOCO_SWITCH: BLOCO_SWITCH EXP_CASE
   75             | %empty

   76 EXP_CASE: TK_CASE EXPRESSAO BLOCO
   77         | TK_DEFAULT BLOCO

   78 CMD_BREAK: TK_BREAK ';'

   79 CMD_CONTINUE: TK_CONTINUE ';'

   80 CMD_OUT: TK_OUT '(' EXPRESSAO ')' ';'

   81 OP_RELACIONAL: '<'
   82              | '>'
   83              | TK_OP_MAIORIGUAL
   84              | TK_OP_MENORIGUAL


