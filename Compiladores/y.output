Gramática

    0 $accept: S $end

    1 S: TK_TIPO TK_MAIN '(' ')' BLOCO

    2 $@1: %empty

    3 BLOCO: '{' $@1 COMANDOS '}'

    4 COMANDOS: COMANDO COMANDOS
    5         | %empty

    6 COMANDO: CMD_DECLARACOES ';'
    7        | CMD_IF
    8        | CMD_WHILE
    9        | CMD_DOWHILE
   10        | CMD_FOR
   11        | CMD_SWITCH
   12        | CMD_BREAK
   13        | CMD_CONTINUE
   14        | CMD_OUT
   15        | CMD_IN
   16        | '#'

   17 COMANDOS_FOR: COMANDO_FOR ',' COMANDOS_FOR
   18             | COMANDO_FOR

   19 COMANDO_FOR: ATRIBUICAO

   20 EXP_SIMPLES: TK_LITERAL
   21            | TK_ID
   22            | '(' EXPRESSAO ')'
   23            | TK_TIPO '(' EXPRESSAO ')'

   24 EXP_NOT: EXP_SIMPLES
   25        | TK_OP_NOT EXP_NOT

   26 EXP_ARITMETICA_MUL: EXP_NOT
   27                   | EXP_ARITMETICA_MUL TK_OP_MUL EXP_NOT

   28 EXP_ARITMETICA_ADD: EXP_ARITMETICA_MUL
   29                   | EXP_ARITMETICA_ADD TK_OP_ADD EXP_ARITMETICA_MUL

   30 EXP_RELACIONAL: EXP_ARITMETICA_ADD
   31               | EXP_RELACIONAL OP_RELACIONAL EXP_ARITMETICA_ADD

   32 EXP_IGUALDADE: EXP_RELACIONAL
   33              | EXP_IGUALDADE TK_OP_IGUALDADE EXP_RELACIONAL

   34 EXP_LOGICAL_AND: EXP_IGUALDADE
   35                | EXP_LOGICAL_AND TK_OP_LOGICAL_AND EXP_IGUALDADE

   36 EXP_LOGICAL_OR: EXP_LOGICAL_AND
   37               | EXP_LOGICAL_OR TK_OP_LOGICAL_OR EXP_LOGICAL_AND

   38 EXPRESSAO: EXP_LOGICAL_OR

   39 ATRIBUICAO: TK_ID '=' EXPRESSAO

   40 CMD_DECLARACOES: TK_VAR LIST_DECLARACOES
   41                | ATRIBUICAO

   42 LIST_DECLARACOES: DECLARACAO ',' LIST_DECLARACOES
   43                 | DECLARACAO

   44 DECLARACAO: TK_ID
   45           | TK_ID '=' EXPRESSAO

   46 CMD_IF: TK_IF '(' EXPRESSAO ')' BLOCO
   47       | TK_IF '(' EXPRESSAO ')' BLOCO TK_ELSE BLOCO

   48 $@2: %empty

   49 CMD_WHILE: TK_WHILE '(' EXPRESSAO ')' $@2 BLOCO

   50 $@3: %empty

   51 CMD_DOWHILE: TK_DO $@3 BLOCO TK_WHILE '(' EXPRESSAO ')'

   52 $@4: %empty

   53 CMD_FOR: TK_FOR $@4 '(' CMD_DECLARACOES ';' EXPRESSAO ';' COMANDOS_FOR ')' BLOCO

   54 $@5: %empty

   55 $@6: %empty

   56 CMD_FOR: TK_FOR $@5 TK_ID '(' EXPRESSAO ',' OP_COMPARATIVOS EXPRESSAO ')' BLOCO $@6

   57 $@7: %empty

   58 CMD_SWITCH: TK_SWITCH '(' TK_ID ')' $@7 '{' BLOCO_SWITCH '}'

   59 BLOCO_SWITCH: BLOCO_SWITCH EXP_CASE
   60             | %empty

   61 EXP_CASE: TK_CASE EXPRESSAO BLOCO
   62         | TK_DEFAULT BLOCO

   63 CMD_BREAK: TK_BREAK ';'

   64 CMD_CONTINUE: TK_CONTINUE ';'

   65 CMD_OUT: TK_OUT '(' EXPRESSAO ')' ';'

   66 CMD_IN: TK_IN '(' TK_TIPO ')' ';'

   67 OP_RELACIONAL: '<'
   68              | '>'
   69              | TK_OP_MAIORIGUAL
   70              | TK_OP_MENORIGUAL

   71 OP_COMPARATIVOS: OP_RELACIONAL
   72                | TK_OP_IGUALDADE
   73                | TK_OP_DESIGUALDADE


